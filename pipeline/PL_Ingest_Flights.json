{
	"name": "PL_Ingest_Flights",
	"properties": {
		"activities": [
			{
				"name": "Copy_Flights_To_Bronze",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET",
						"paginationRules": {
							"supportRFC5988": "true"
						}
					},
					"sink": {
						"type": "JsonSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "JsonWriteSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "DS_OpenSky_States",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DS_ADLS_Bronze_Flights",
						"type": "DatasetReference",
						"parameters": {
							"TargetFileName": {
								"value": "@concat('flights_', formatDateTime(utcNow(), 'yyyyMMdd_HHmmss'), '.json')",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"annotations": []
	}
}